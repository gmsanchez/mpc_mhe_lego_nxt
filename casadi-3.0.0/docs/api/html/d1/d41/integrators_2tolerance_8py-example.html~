<div class="line"><a name="l00061"></a><span class="lineno">   61</span>&#160;  opts[<span class="stringliteral">&#39;reltol&#39;</span>] = tol</div>
<div class="line"><a name="l00062"></a><span class="lineno">   62</span>&#160;  opts[<span class="stringliteral">&#39;abstol&#39;</span>] = tol</div>
<div class="line"><a name="l00063"></a><span class="lineno">   63</span>&#160;  opts[<span class="stringliteral">&#39;tf&#39;</span>] = tend</div>
<div class="line"><a name="l00064"></a><span class="lineno">   64</span>&#160;  F = <a class="code" href="../../dd/d1b/group__integrator.html#gaeff67b23fd72e2f42dc41a35fa6a2441">integrator</a>(<span class="stringliteral">&#39;F&#39;</span>, <span class="stringliteral">&#39;cvodes&#39;</span>, dae, opts)</div>
