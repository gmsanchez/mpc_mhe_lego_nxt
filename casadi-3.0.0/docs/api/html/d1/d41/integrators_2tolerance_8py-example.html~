<div class="line"><a name="l00056"></a><span class="lineno">   56</span>&#160;tolerances = logspace(-15,1,500)</div>
<div class="line"><a name="l00057"></a><span class="lineno">   57</span>&#160;endresult=[]</div>
<div class="line"><a name="l00058"></a><span class="lineno">   58</span>&#160;</div>
<div class="line"><a name="l00059"></a><span class="lineno">   59</span>&#160;<span class="keywordflow">for</span> tol <span class="keywordflow">in</span> tolerances:</div>
