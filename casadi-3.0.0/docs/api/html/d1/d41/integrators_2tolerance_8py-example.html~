<div class="line"><a name="l00030"></a><span class="lineno">   30</span>&#160;x=SX.sym(<span class="stringliteral">&#39;x&#39;</span>) </div>
<div class="line"><a name="l00031"></a><span class="lineno">   31</span>&#160;dx=SX.sym(<span class="stringliteral">&#39;dx&#39;</span>)</div>
<div class="line"><a name="l00032"></a><span class="lineno">   32</span>&#160;states = vertcat(x,dx)</div>
<div class="line"><a name="l00033"></a><span class="lineno">   33</span>&#160;</div>
<div class="line"><a name="l00034"></a><span class="lineno">   34</span>&#160;dae={<span class="stringliteral">&#39;x&#39;</span>:states, <span class="stringliteral">&#39;ode&#39;</span>:vertcat(dx,-x)}</div>
<div class="line"><a name="l00035"></a><span class="lineno">   35</span>&#160;</div>
<div class="line"><a name="l00036"></a><span class="lineno">   36</span>&#160;tend = 2*pi*3</div>
<div class="line"><a name="l00037"></a><span class="lineno">   37</span>&#160;ts = linspace(0,tend,1000)</div>
<div class="line"><a name="l00038"></a><span class="lineno">   38</span>&#160;</div>
<div class="line"><a name="l00039"></a><span class="lineno">   39</span>&#160;tolerances = [-10,-5,-4,-3,-2,-1]</div>
<div class="line"><a name="l00040"></a><span class="lineno">   40</span>&#160;</div>
<div class="line"><a name="l00041"></a><span class="lineno">   41</span>&#160;figure()</div>
<div class="line"><a name="l00042"></a><span class="lineno">   42</span>&#160;</div>
<div class="line"><a name="l00043"></a><span class="lineno">   43</span>&#160;<span class="keywordflow">for</span> tol <span class="keywordflow">in</span> tolerances:</div>
<div class="line"><a name="l00044"></a><span class="lineno">   44</span>&#160;  opts = {<span class="stringliteral">&#39;reltol&#39;</span>:10.0**tol, <span class="stringliteral">&#39;abstol&#39;</span>:10.0**tol, <span class="stringliteral">&#39;grid&#39;</span>:ts, <span class="stringliteral">&#39;output_t0&#39;</span>:<span class="keyword">True</span>}</div>
<div class="line"><a name="l00045"></a><span class="lineno">   45</span>&#160;  F = <a class="code" href="../../dd/d1b/group__integrator.html#gaeff67b23fd72e2f42dc41a35fa6a2441">integrator</a>(<span class="stringliteral">&#39;F&#39;</span>, <span class="stringliteral">&#39;cvodes&#39;</span>, dae, opts)</div>
